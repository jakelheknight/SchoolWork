<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="346.0" prefWidth="416.0">
         <top>
            <Label text="Jacob Knight" BorderPane.alignment="CENTER">
               <font>
                  <Font name="AdobeFanHeitiStd-Bold" size="38.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <TextField fx:id="myTextField" prefHeight="78.0" prefWidth="416.0" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <VBox alignment="CENTER" opacity="0.9" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="myButton" mnemonicParsing="false" onAction="#execute" text="Execute">
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding></Button>
                  <Label fx:id="output" text="Output">
                     <font>
                        <Font size="30.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="valitity" text="Label">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <Glow level="0.5" />
               </effect>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
